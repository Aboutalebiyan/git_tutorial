      "source": [
        "import re\n",
        "\n",
        "def is_palindrome(phrase):\n",
        "    forwards = ''.join(re.findall(r'[a-z1-9]+', phrase.lower()))\n",
        "    backwards = forwards[::-1]\n",
        "    return forwards == backwards"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "print(is_palindrome(\"Go hang a salami, I'm a lasagna hog.\"))\n",
        "print(is_palindrome(\"123454321\"))"
      ],
